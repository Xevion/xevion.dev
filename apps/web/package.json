    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "p5": "^1.11.7",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.5",
    "svelte-wrap-balancer": "^0.0.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
