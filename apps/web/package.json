{
  "name": "@xevion/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 5000",
    "build": "vite build",
    "preview": "vite preview --port 5000",
    "start": "node build/index.js",
    "prepare": "svelte-kit sync || echo ''",
