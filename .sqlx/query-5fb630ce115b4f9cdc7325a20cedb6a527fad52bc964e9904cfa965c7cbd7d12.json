  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            t.id, \n            t.slug, \n            t.name,\n            t.color,\n            t.created_at,\n            COUNT(pt.project_id)::int as \"project_count!\"\n        FROM tags t\n        LEFT JOIN project_tags pt ON t.id = pt.tag_id\n        GROUP BY t.id, t.slug, t.name, t.color, t.created_at\n        ORDER BY t.name ASC\n        ",
